{
    "name": "karuta",
    "displayName": "Karuta",
    "description": "Karuta syntax support",
    "publisher": "karuta",
    "version": "0.0.6",
    "engines": {
        "vscode": "^1.37.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [{
            "id": "karuta",
            "aliases": ["Karuta", "karuta"],
            "extensions": [".karuta"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "karuta",
            "scopeName": "source.karuta",
            "path": "./syntaxes/karuta.tmLanguage.json"
        }]
    },
    "repository": {
	"type": "git",
	"url": "https://github.com/nlsynth/karuta.git"
    }
}
