#! /bin/sh

# Wrapper script of karuta-bin to be installed.
# (the original file is karuta.in and karuta is generated by configure script)

prefix=@prefix@

export KARUTA_DIR=@datadir@/karuta
export IROHA_DATA_DIR=@datadir@/iroha

PATH=@prefix@/bin:$PATH

if test "${GDB+set}" = set; then
  GDB="$GDB --args"
fi

$GDB karuta-bin $@
